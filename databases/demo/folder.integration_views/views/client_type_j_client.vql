CREATE OR REPLACE VIEW client_type_j_client FOLDER = '/integration_views' AS SELECT client_type.code AS code, client_type.value AS value, client.client_id AS client_id, client.name AS name, client.surname AS surname, client.client_type AS client_type FROM client AS client INNER JOIN client_type AS client_type ON client.client_type = client_type.code ;

ALTER VIEW client_type_j_client
 LAYOUT (client_type = [300, 63, 200, 200], client = [69, 91, 200, 200]);

